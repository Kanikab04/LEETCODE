class Solution {
public:
    void sortColors(vector<int>& nums) {
        int red=0,white=1,blue=2;
        for(int i=0;i<nums.size();i++){
            for(int j=0;j<nums.size();j++){
                if(nums[i]<nums[j]){
                    swap(nums[j],nums[i]);
                }
            }
        }
        
       
    }
};
